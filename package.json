{
  "name": "send-action",
  "version": "2.0.2",
  "description": "simple state management",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test:node": "node test.js | tap-spec",
    "test": "npm run lint && npm run test:node",
    "build:local": "minidocs docs -c docs/content.json -o dist --pushstate",
    "build:deploy": "minidocs docs -c docs/content.json -b /send-action -o dist --full-html",
    "start": "budo --dir dist --pushstate",
    "deploy": "npm run build:deploy && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sethvincent/send-action.git"
  },
  "keywords": [
    "state",
    "store",
    "flux",
    "redux",
    "unidirectional",
    "state management"
  ],
  "author": "sethvincent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sethvincent/send-action/issues"
  },
  "homepage": "https://github.com/sethvincent/send-action#readme",
  "dependencies": {
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "minidocs": "github:freeman-lab/minidocs",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "virtual-dom": "^2.1.1",
    "virtual-raf": "^3.0.0",
    "yo-yo": "^1.3.1"
  }
}
